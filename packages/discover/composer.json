{
  "name": "quellabs/discover",
  "description": "Service discovery component for the Quellabs ecosystem",
  "type": "composer-plugin",
  "license": "MIT",
  "keywords": [
    "quellabs",
    "service-discovery"
  ],
  "authors": [
    {
      "name": "Floris van den Berg",
      "email": "support@quellabs.com"
    }
  ],
  "config": {
    "platform": {
      "php": "8.2"
    }
  },
  "require": {
    "ext-ctype": "*",
    "quellabs/support": "^1.",
    "quellabs/contracts": "^1.",
    "composer-plugin-api": "^2.0",
    "monolog/monolog": "*"
  },
  "require-dev": {
    "composer/composer": "^2.0"
  },
  "autoload": {
    "psr-4": {
      "Quellabs\\Discover\\": "src/"
    }
  },
  "extra": {
    "class": "Quellabs\\Discover\\Composer\\ServiceDiscoveryPlugin"
  }
}