{
  "name": "quellabs/canvas-database",
  "description": "CakePHP Database integration for Canvas framework",
  "type": "library",
  "license": "MIT",
  "require": {
    "php": "^8.1",
    "quellabs/contracts": "^1.0",
    "quellabs/dependency-injection": "^1.0",
    "cakephp/database": "^5.0"
  },
  "autoload": {
    "psr-4": {
      "Quellabs\\CanvasDatabase\\": "src/"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "extra": {
    "discover": {
      "di": {
        "provider": "Quellabs\\CanvasDatabase\\Discovery\\ServiceProvider",
        "config": [
          "config/database.php"
        ]
      }
    }
  }
}